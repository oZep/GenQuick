{
    "manifest_version": 2,
    "name": "GenQuick",
    "version": "1.0",
    "description": "A chrome extension that enable Genshin Impact speedrunners to have access to everything they need with 1 click.",
    "icons": {
      "16": "src/img/GenQuickLogo.png",
      "48": "src/img/GenQuickLogo.png",
      "128": "src/img/GenQuickLogo.png"
    },
    "browser_action": {
      "default_icon": {
        "16": "src/img/GenQuickLogo.png",
        "48": "src/img/GenQuickLogo.png",
        "128": "src/img/GenQuickLogo.png"
      },
      "default_popup": "src/popup.html"
    },
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["src/js/content.js"]
      }
    ]
  }
  